{
    "name": "Focalboard",
    "id": "focalboard",
    "packaging_format": 1,
    "description": {
        "en": "Self-hosted alternative to Trello, Notion, and Asana",
        "fr": "Alternative auto-hébergée à Trello, Notion et Asana "
    },
    "version": "0.6.1~ynh1",
    "url": "https://www.focalboard.com/",
    "license": "MIT",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com",
        "url": "http://example.com"
    },
    "requirements": {
        "yunohost": ">= 4.1.7"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/focalboard",
                "default": "/focalboard"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
